{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "regions": ["cdg1"],
  "env": {
    "NEXT_PUBLIC_WEBSOCKET_URL": "@websocket_url"
  },
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/socketio"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}